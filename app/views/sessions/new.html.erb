<h1>Sign in</h1>

<%= form_with url: sign_in_path do |form|%>

    <div class="mb-3">
        <%= form.label :Email %>
        <%= form.text_field :Email, class:"form-control", placeholder: "jane@tamu.edu" %>
    </div>

    <div class="mb-3">
        <%= form.label :Password %>
        <%= form.password_field :Password, class:"form-control", placeholder: "Password" %>
    </div>

    <div class="mb-3">
        <%= form.submit "Sign in", class:"btn btn-primary" %>
    </div>

<%end%>